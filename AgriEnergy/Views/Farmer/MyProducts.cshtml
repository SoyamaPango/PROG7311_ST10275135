@using AgriEnergy.ViewModels
@model FarmerProductsViewModel


<h2>My Products</h2>

@if (Model?.Products != null && Model.Products.Any())
{
    <ul>
        @foreach (var product in Model.Products)
        {
            <li>@product.Name - @product.Category</li>
        }
    </ul>
}
else
{
    <p>No products found.</p>
}
